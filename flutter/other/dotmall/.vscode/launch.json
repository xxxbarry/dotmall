    "version": "0.2.0",
    "configurations": [
        {
            "name": "dotmall",
            "cwd": "dotmall",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "dotmall (profile mode)",
            "cwd": "dotmall",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "dotmall (release mode)",
            "cwd": "dotmall",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "-- Generator build",
            "request": "launch",
            "program": "packeges/dotmall_sdk/.dart_tool/build/entrypoint/build.dart",
            "type": "dart",
            "args": ["build","--delete-conflicting-outputs"]
        },
        {
            "name": "-- Generator watch",
            "request": "launch",
            "program": "packeges/dotmall_sdk/.dart_tool/build/entrypoint/build.dart",
            "type": "dart",
            "args": ["watch","--delete-conflicting-outputs"]
        }
    ]
}